					"target": [
						"OutboundReqDetail",
						"OutboundReqMaster"
					]
				}, {
					"pattern": "OutboundReqDetail/{xblnr_mkpf}",
					"name": "OutboundReqDetail",
					"target": [
						"OutboundReqDetail",
						"OutboundReqMaster"
					]
				}, {
					"pattern": "inbound",
					"name": "inbound",
					"target": [
						"InboundDetail",
						"InboundMaster"
					]
				}, {
					"pattern": "InboundMaster",
					"name": "InboundMaster",
					"target": [
						"InboundDetail",
						"InboundMaster"
					]
				}, {
					"pattern": "InboundDetail/{vbeln}",
					"name": "InboundDetail",
					"target": [
						"InboundMaster",
						"InboundDetail"
					]
				}, {
					"pattern": "PostReceipt",
					"name": "PostReceipt",
					"target": [
						"PostReceiptDetail",
						"PostReceiptMaster"
					]
				}, {
					"pattern": "PostReceiptMaster",
					"name": "PostReceiptMaster",
					"target": [
						"PostReceiptDetail",
						"PostReceiptMaster"
					]
				}, {
					"pattern": "PostReceiptDetail/{vbeln}",
					"name": "PostReceiptDetail",
					"target": [
						"PostReceiptMaster",
						"PostReceiptDetail"
					]
				}
			]
