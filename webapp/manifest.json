				}, {
					"name": "invoicesDetail",
					"pattern": "invoicesMaster/{vbeln}/{shipto}",
					"titleTarget": "",
					"greedy": false,
					"target": ["invoicesMaster", "invoicesDetail"]
				},

				{
					"pattern": "PosReq",
					"name": "PosReq",
					"target": [
						"PosReqDetail",
						"PosReqMaster"
					]
				}, {
					"pattern": "PosReqMaster",
					"name": "PosReqMaster",
					"target": [
						"PosReqDetail",
						"PosReqMaster"
					]
				}, {
					"pattern": "PosReqDetail/{banfn}",
					"name": "PosReqDetail",
					"target": [
						"PosReqDetail",
						"PosReqMaster"
					]
				}, {
					"pattern": "OutboundRequest",
					"name": "OutboundRequest",
